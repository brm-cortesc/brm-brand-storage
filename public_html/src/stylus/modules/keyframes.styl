@keyframes toCircle	
	50%
		border-radius 10px
		height 4.2rem
		width 4.2rem
		
	100%
		border-radius 100%
		height 4.2rem
		width 4.2rem		


@keyframes zoomInOut
	from
		opacity 0
		transform scale(0)
	
	to
		opacity 0.99
		transform scale(1)



@keyframes dateIn
	from
		opacity 0
		transform translate3d(-50%,-25%,0)
		
	to
		opacity 0.999
		transform translate3d(-50%,-75%,0)
		

@keyframes backfadeIn
	from
		opacity 0
		
	to
		opacity 0.8

@keyframes modalIn
	from
		opacity 0
		transform translate(-50%, 95%)
		
	to
		opacity 0.999
		transform translate(-50%, 0)